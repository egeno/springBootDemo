@charset "utf-8";

/* CSS Document */

html,
body,
div,
ul,
ol,
li,
p,
h3,
h4,
span,
input,
textarea,
select,
option,
table,
tr,
td,
h2,
h1,
dl,
dt,
dd,
h5 {
	padding: 0;
	margin: 0;
	box-sizing: border-box;
	-o-box-sizing: border-box;
	-ms-box-sizing: border-box;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
}
input {      
     -webkit-user-select:auto; /*webkit浏览器*/     
}  
*{   
    -webkit-touch-callout:none;  
    -webkit-user-select:none;    
    -khtml-user-select:none;   
    -moz-user-select:none;  
    -ms-user-select:none;   
    user-select:none;   
    
}  
* {-webkit-tap-highlight-color:rgba(0,0,0,0);}
body {
	min-width: 3.2rem;
	max-width: 6.4rem;
	font-family: "黑体", "microsoft yahei";
	height: 100%;
	margin: 0 auto;
	color: #000;
	-moz-text-size-adjust: none;
	-webkit-text-size-adjust: none;
	text-size-adjust: none;
	font-size: 0.24rem;
	-webkit-overflow-scrolling: touch;
	/* ios5+ */
	background:#F5F5F5;
	
}
.bg{
	background:#fff;
}


img {
	border: 0 none;
}

html {
	font-size: 100px;
	height: 100%;
}

ul,
li {
	list-style-type: none;
}

input,
textarea {
	outline: none;
	background: none;
	border: 0 none;
	border-radius: 0;
}

a {
	text-decoration: none;
}

em,i{
	font-style: normal;
}

a,input,img{
	outline:none;
}
.cle:after{visibility:hidden; display:block; font-size:0; content:'\20'; clear:both; height:0;}
.clear { clear: both; height: 0; font-size: 0; line-height: 0; overflow: hidden; }
.cle:after, .clearfix:after, .clear_f:after, .cle_float:after { visibility: hidden; display: block; font-size: 0; content: '\20'; clear: both; height: 0; }
.cle, .clearfix, .clear_f, .cle_float { *zoom: 1 }
.lf { float: left; }
.lr { float: right; }
.clearfix { clear: both; }

.disabled{
	pointer-events: none;
}
.index_main,.order_content{
	width: 100%;
	min-width: 3.2rem;
	max-width: 6.4rem;
	margin: 0 auto;
}
.splitLine{
	width:100%;
	height:0.19rem;
	background:#f5f5f5;
	border-top:1px solid #e6e6e6;
	border-bottom:1px solid #e6e6e6;
}
.title{
	height:0.9rem;
	line-height:0.9rem;
	text-align:center;
	color:#1a1a1a;
	font-size:0.38rem;
	overflow:hidden;
	padding:0 0.25rem;
	background:#fff;
}
.title img{
	width:0.25rem;
	height:0.45rem;
	float:left;
	margin-top:0.2rem;
}

/* 弹窗 */
.dialog{
	position: fixed;
	top:0;
	width:100%;
	height:100%;
	min-width: 3.2rem;
	max-width: 6.4rem;
	background:rgba(0,0,0,0.5);
	z-index:9999;
	display:none;
	left:50%;
	margin-left:-3.2rem;
}
.dialog_box{
    width: 5.50rem;
    height: 2.60rem;
    background: #fff;
    border-radius: 0.3rem;
    text-align: center;
    position: absolute;
    bottom:50%;
    left:50%;
    margin-bottom: -1.8rem;
    margin-left: -2.75rem;
}
.dialog_box h3{
	font-size:0.36rem;
	font-weight: 800;
	margin-top: 0.5rem;
}
.dialog_box p{
	font-size:0.3rem;
	line-height: 0.5rem;
}
.dialog_box .know{
    position: absolute;
    bottom: 0px;
    line-height: 0.9rem;
    text-align: center;
    width: 100%;
    color: #2e91fb;
    font-size: 0.34rem;
    border-top: 1px solid #d8d8d8;
}
.foot{
	min-width: 3.2rem;
	max-width: 6.4rem;
	width:100%;
	position:fixed;
	bottom:0;
	border-top:1px solid #cecece;
	padding: 0.1rem 0 0.01rem 0;
	background:#fff;
	z-index:999;
}
.foot ul{
	width:100%;
}
.foot li{
	float:left;
	/* width:33.33%; */
	width:20%;
	text-align:center;
}
.foot li img{
	width:0.54rem;
	height:0.5rem;
}
.foot li span{
	display:block;
	font-size:0.24rem;
	color:#666;
	margin-bottom: 0.02rem;
	
}
.foot li.active span{
	color:#ff8420;
}
.foot li.active .one{
	display:none;
}
.foot li.active .two{
	display:inline-block;
}
.foot li .one{
	display:inline-block;
}
.foot li .two{
	display:none;
}
.disable{
	pointer-events: none;
}

