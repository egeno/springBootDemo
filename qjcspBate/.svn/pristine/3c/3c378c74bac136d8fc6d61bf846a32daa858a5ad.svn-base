@charset "utf-8";
/* CSS Document */
html,body,div,ul,ol,li,img,p,h3,h4,span,input,textarea,select,option,table,tr,td,h2,h1,dl,dt,dd,h5 {
	padding: 0;
	margin: 0;
}
html{
	width:100%;
	height:100%;
	font-size:100px;
}
body{
	width:100%;
	height:100%;
	overflow:hidden;
	font: normal 100% Helvetica, Arial, sans-serif;
	font-family:"Microsoft YaHei";
}
img {
	border: none;
}
ul,li,ol {
	list-style: none;
}
input,textarea {
	outline: none;
	background: none;
	border: 0 none;
	border-radius: 0;
}
a {
	text-decoration: none;
	outline: none;
}
em{
	font-style: normal;
}
.total{
	width:100%;
	height:100%;
}
/* ͷ�� */
.top {
    position: absolute;
    top: 0px;
    left: 0px;
    height: 50px;
    background: #2aa2ed;
    width: 100%;
}

.top .logo{
	font-size:14px;
	float:left;
	margin-left:12px;
	margin-top:7px;
}
.top .topRight{
	float:right;
	height:50px;
	line-height:50px;
}
.top .topRight>ul>li{
	float:right;
	padding:0 20px 0 35px;
}
.top .topRight>ul>li>a{
	display:block;
	font-size:14px;
	color:#ffffff;
	line-height:50px;
}
.top .logo img{
	width:209px;
	height:36px;
	color:#ffffff;
	line-height:50px;
}
.top .topRight li.user{
	position:relative;
	padding:0;
	width:130px;
	text-align:center;
	background:url(../img/user2.png) no-repeat center;
	background-size:contain;
	line-height:50px;
}
.top .topRight li.refresh{
	background:url(../img/refresh.png) no-repeat center left;
	line-height:50px;
}
.top .topRight li.info{
	background:url(../img/info.png) no-repeat center left;
	line-height:50px;
}
.top .topRight li.user ul.userName{
	position:absolute;
	top:50px;
	background:#fff;
	right:0;
	width:129px;
	border:1px solid #d2d2d2;
	box-shadow:-3px 3px 5px #d2d2d2;
	display:none;
	z-index:100;
}
.userName li:last-child{
	border:0 none;
}
.userName li{
	line-height:45px;
	height:45px;
	border-bottom:1px solid #d2d2d2;
}
.userName a{
	display:block;
	text-align:center;
	color:#369fe1;
	font-size:14px;
}

/* ����  */
.content{
	padding-top:05px;
	overflow:hidden;
	height:100%;
	width:100%;
}
.content .contentLeft{
	float:left;
	background:#384556;
	width:45px;
	height:100%;
}
.menu{
	padding-top:09px;
	text-align:center;
}
.menu img{
	width:20px;
	height:15px;
}
.content .contentRight{
	float:right;
	background:#fcfcfc;
	width:100%;
	height:100%;
}
.note{
	width:100%;
	height:36px;
	border-bottom:1px solid #bebebe;
	font-size:14px;
	color:#1a1a1a;
	line-height:36px;
	padding-left:0pxm;
	background:#ebebeb;
}
.nav,.accoun,.message{
	width:100%;
	margin:0 auto;
} 
.nav{
	height:40px;
	margin-bottom:0pxm;
	border-bottom:1px solid #d9d9d9;
}
.accoun{
	height:174px;
	margin-bottom:0pxm;
}
.personal{
	/* float:left; */
	background:url(../img/personal.png) no-repeat;
	background-size:100% 100%;
	width:36%;
	height:174px;
	position:relative;
}
.personal p{
	font-size:14px;
	color:#1a1a1a;
}
.name{
	position:absolute;
	top:55px;
	left:135px;
	font-size:12px;
	color:#1a1a1a;
}
.personal em{
	display:block;
	width:74px;
	height:75px;
	position:absolute;
	left:40px;
	top:45px;
	background:url(../img/personal2.png) no-repeat center;
	background-size:contain;
}
.out{
	position:absolute;
	right:35px;
	top:75px;
}
.personal p{
	position:absolute;
	left:135px;
	top:90px;
}
.system{
	width:60%;
	float:right;
	background:url(../img/system.png) no-repeat;
	background-size:100% 100%;
	height:174px;
}
.system h1{
	font-size: 16px;
    color: #1a1a1a;
    margin-top: 10px;
    margin-bottom: 35px;
    margin-left: 30px;

}
.system p{
	font-size:14px;
	color:#1a1a1a;
	margin-left:30px;
	line-height:35px;
}
.message{
	/*height:7.65rem;*/
	/*border:1px solid #bfbfbf;*/
	/*border-radius:5px;*/
	height:69%;
	overflow:hidden;
	background:url(../img/bg.jpg) no-repeat left top;
	background-size:cover;
}
.foot{
	text-align:center;
}


table {
	clear: both; /*border-collapse: collapse;*/
	border: 1px solid #c9dae4;
}

table tr th {
	width:120px;
	color: #000;
	padding: 5px 10px 5px 0px;
	border-top: 1px solid #e6e6e6;
	border-bottom: 1px solid #e6e6e6;
	font-weight: normal;
	background: #f7fafc;
	text-align: right;
	border-right: 1px solid #e6e6e6;
	border-left: 1px solid #e6e6e6;
	font-size: 14px;
}

table tr td {
	width:180px;
	border-bottom: 1px solid #e6e6e6;
	padding: 5px 0px 5px 10px;
	line-height: 22px;
	word-break: break-all;
}

table tr th em, table tr td em {
	color: Red;
	font-size: 12px;
	padding-left: 5px;
}

table tr td .easyui-textbox{
	height: 22px;
	width: 170px;
	line-height: 16px;
    border-radius: 3px 3px 3px 3px;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075) inset;
    transition: border 0.2s linear 0s, box-shadow 0.2s linear 0s;
    font-size: 13px;
}

table tr td .easyui-numberbox{
	height: 22px;
	width: 170px;
	line-height: 16px;
    border-radius: 3px 3px 3px 3px;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075) inset;
    transition: border 0.2s linear 0s, box-shadow 0.2s linear 0s;
    font-size: 13px;
    border: 1px solid #D4D4D4;
	vertical-align: middle;
}

.gallery:after,
.gallery:before {
	display: table;
	content:"";
}
.gallery:after {
	clear:both;
	overflow: hidden;
}
.gallery {
	list-style: none;
	margin: 0;
	padding: 0;
	zoom:1;
}

.gallery img {
	background: #fff;
	border: solid 1px #ccc;
	padding: 4px;
	width: 125px;
	height: 112px;
}
@media screen and (max-width: 1401px) { 
.content .contentRight {width: 1320px;} 
.accoun {width: 1260px;} 
.personal {width: 617px;} 
.system {width: 460px;} 
} 