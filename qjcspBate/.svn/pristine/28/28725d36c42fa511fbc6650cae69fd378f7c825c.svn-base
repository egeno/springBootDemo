package com.qjkj.qjcsp.entity.order;
/**
 * 商品表
 * @author yehx
 * @date 2016年1月4日  上午11:39:13
 */
public class Goods {
	private Long    goodsId;//商品id
	private String  goodsName;//商品名称
	private Integer goodsNum;//商品数量
	private String  itemGrade;//商品打分
	private String  commentContent;//商品评论内容
	
	public String getGoodsName() {
		return goodsName;
	}
	public void setGoodsName(String goodsName) {
		this.goodsName = goodsName;
	}
	public Integer getGoodsNum() {
		return goodsNum;
	}
	public void setGoodsNum(Integer goodsNum) {
		this.goodsNum = goodsNum;
	}
	
	public Long getGoodsId() {
		return goodsId;
	}
	public void setGoodsId(Long goodsId) {
		this.goodsId = goodsId;
	}
	public String getItemGrade() {
		return itemGrade;
	}
	public void setItemGrade(String itemGrade) {
		this.itemGrade = itemGrade;
	}
	public String getCommentContent() {
		return commentContent;
	}
	public void setCommentContent(String commentContent) {
		this.commentContent = commentContent;
	}
	@Override
	public String toString() {
		return "Goods [goodsId=" + goodsId + ", goodsName=" + goodsName + ", goodsNum=" + goodsNum + ", itemGrade="
				+ itemGrade + ", commentContent=" + commentContent + "]";
	}
	
	
}
