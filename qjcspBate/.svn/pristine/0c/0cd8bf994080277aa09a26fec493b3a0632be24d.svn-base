body{color:#2f2f2f;/*overflow:hidden;height:100%;*/}
.splitLine{
	background:#f5f5f5;
	height:0.2rem;
	border-bottom:1px solid #d8d8d8;
    border-top:1px solid #d8d8d8;
}
.order_content{
	padding-bottom:1rem;
}
.takePlace{
	padding-left:0.24rem;
	margin: 0.1rem 0px;
}
.takePlace h1{
	border-bottom:1px solid #d8d8d8;
	text-indent:0.06rem;
	font-size:0.32rem;
	color:#444;
	height:0.85rem;
	line-height:0.9rem;
	font-weight:normal;
}
.takePlace p{
	text-indent:0.06rem;
	height:0.85rem;
	line-height:0.9rem;
	color:#ccc;
	font-size:0.26rem;
	width:100%;
	white-space:nowrap; overflow:hidden; text-overflow:ellipsis;
}
.packet,.phone{
	height:0.85rem;
	line-height:0.9rem;
	overflow:hidden;
	padding:0 0.3rem;
}
.packet h1,.phone h1{
	float:left;
	font-size:0.32rem;
	color:#444;
	font-weight:normal;
}
.packet p,.phone p{
	float:right;
	font-size:0.28rem;
	color:#ccc;
}
.packet img{
	float:right;
	width:0.13rem;
	height:0.26rem;
	margin-top:0.32rem;
	margin-left:0.12rem;
}
.orderList{
	background:#fff;
}
.reserve {
    line-height: 0.5rem;
    background: #eee;
    padding-left: 0.25rem;
    border-bottom: 1px solid #ccc;
}
.reserve .date {
    margin-right: 0.1rem;
}
.orderList li {
    line-height: 0.5rem;
    border-bottom: 1px solid #eee;
    font-size: 0.28rem;
    padding-left: 0.25rem;
}
.orderList .dishName {
    width: 4rem;
    display: inline-block;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    font-size: 0.28rem;
    
}
.orderList span{
	margin-top: 0.14rem;
}
.orderList .dishNum {
    width: 0.6rem;
    display: inline-block;
    overflow: hidden;
    margin-left: 0.2rem;
}
.orderList .dishPrice {
    float: right;
    margin-right: 0.25rem;
}
.orderList h1{
	border-left:0.08rem solid #fd8424;
	height:0.5rem;
	line-height:0.55rem;
	border-bottom:1px solid #d8d8d8;
	background:#e6e6e6;
	font-size:0.24rem;
	color:#808080;
	font-weight:normal;
}
.orderList h1 span{
	margin-left:0.15rem;
}
.orderList p{
	margin-left:0.25rem;
	text-indent:0.06rem;
	height:0.85rem;
	line-height:0.9rem;
	border-top:1px solid #d8d8d8;
}
.orderList p:nth-child(0){border:none;}
.orderList p span{
	font-size:0.32rem;
	color:#444;
	display:inline-block;
}
.goodsName{
	width:60%;
	white-space:nowrap; overflow:hidden; text-overflow:ellipsis;
}
.goodsName img{
	width:0.35rem;
	height:0.35rem;
	vertical-align: sub;
	margin-left: 0.15rem;
}
.goodsCount{
	width:10%;
	text-align:center;
	margin-right:2%;
	position: absolute;
}
.goodsPrice{
	width:25%;
	text-align:center;
	position: absolute;
    right: 0.2rem;
}
.order_bottom{
	position:fixed;
	bottom:0;
	background:#3d3d3d;
	min-width: 3.2rem;
	max-width: 6.4rem;
	width:100%;
	z-index:999;
	height:1rem;
	overflow:hidden;
}
.order_bottom span{
	display:inline-block;
}
.orderMoney{
	font-family:"微软雅黑";
	width:4.4rem;
	float:left;
	font-size:0.26rem;
	color:#fff;
	line-height:1rem;
	text-align:right;
}
.orderMoney span{
	float:right;
}
.orderMoney em{
	float:right;
	font-size:0.38rem;
	display:inline-block;
	margin-left:0.1rem;
	margin-right:0.3rem;
}
.orderMoney i{
	margin-left:0.1rem;
}
.orderSubmit a{
	width:2rem;
	float:right;
	text-align:center;
	background:#fd8432;
	color:#fff;
	font-size:0.34rem;
	height:1rem;
	line-height:1rem;
}
.order{
	height:0.32rem;
}
/*优惠*/
.discount{
	font-size:0.32rem;
	padding-left:0.25rem;
	margin-bottom: 0.1rem;
}
.discount h1{
	font-size:0.32rem;
	color:#999;
	font-weight: 400;
	line-height: 0.9rem;
	border-bottom: 1px solid #d8d8d8;
}
.discount_down p{
	line-height: 0.9rem;
	border-bottom: 1px solid #d8d8d8;
}
.discount_down img{
	width:0.36rem;
	height:0.36rem;
	vertical-align: sub;
}
.reduceMoney{
	color:#fd8432;
	margin-right:0.25rem;
}
.shouldpay{
	text-align: right;
	margin-right:0.25rem;
	line-height: 0.9rem;
	border-bottom: 1px solid #d8d8d8;
}
.shouldpay span{
	color:#f8504a;
	font-size: 0.4rem;
}
.discountMoney{
	position: absolute;
	left:0.25rem;
	line-height: 1rem;
	font-size:0.26rem;
	color:#a9a9a9;
}




