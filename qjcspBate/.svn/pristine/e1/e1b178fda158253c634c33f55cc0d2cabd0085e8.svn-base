.change_content{
	
}
img.searchSign{
	width:0.28rem;
	height:0.28rem;
	float:left;
	margin:0.14rem 0.1rem 0 0.15rem;
	float:left;
}
.change{
	background:#fff;
	padding-bottom:0.34rem;
}
.change_search{
	width:5.78rem;
	height:0.58rem;
	border:1px solid #f3f3f3;
	background:#f6f6f6;
	border-radius:5px;
	margin-left: 0.31rem;
	line-height:0.58rem;
}
.change_search input{
	font-size:0.3rem;
	color:#000;
	height: 0.35rem;
    line-height: 0.365rem;
	float:left;
	margin-top: 0.15rem;
}
.location{
	height:0.9rem;
	line-height:0.9rem;
	text-indent:0.1rem;
	background:#fff;
	text-align:center;
}
.location span{
	font-size:0.3rem;
	color:#333;
	display: inline-block;
	height:0.32rem;
	line-height:0.32rem;
}
.location img{
	width:0.32rem;
	height:0.32rem;
	display: inline-block;
	vertical-align: sub;
}
.history h1{
	font-size:0.28rem;
	color:#333;
	height:0.7rem;
	line-height:0.7rem;
	border-top:1px solid #e6e6e6;
	border-bottom:1px solid #e6e6e6;
	background:#F5F5F5;
	font-weight:normal;
	padding:0 0.3rem;
}
.history p{
	padding:0.2rem 0.3rem;
	font-size:0.24rem;
	color:#808080;
	line-height:0.32rem;
	border-bottom:1px solid #e6e6e6;
	background:#fff;
}
.clearHistory{
	text-align:center;
	font-size:0.26rem;
	color:#808080;
	margin-top:0.5rem;
}
/* 弹窗 */
.dialog_clear h3{
	margin-top: 0.3rem;
}
.dialog_clear p{
	margin-bottom: 0.2rem;
}
.dialog_clear{
	width:4.7rem;
	height:2.23rem;
	margin-left:-2.35rem;
}
.btn{
	border-top: 1px solid #d8d8d8;
	height:0.83rem;
	line-height:0.83rem;
}
.btn span{
	float:left;
	text-align: center;
	font-size:0.3rem;
	color:#2e91fb;
	font-weight:bold;
	width:49.9%;
}
.btn span.sure{
	border-right:1px solid #d8d8d8;
}
.buttom{
	width: 1.2rem;
    height: 0.6rem;
    background: #ff8420;
    line-height: 0.6rem;
    text-align: center;
    color: #fff;
    display: block;
    border-radius: 7px;
    -moz-border-radius: 7px;
    -khtml-border-radius: 7px;
    overflow: hidden;
    position: absolute;
    right: 0.15rem;
    display:none;
      float:left;
}
