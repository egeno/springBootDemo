$(document).ready(function() {
    var balance = localStorage.getItem("balance");
    $(".mymoney span").empty();
    $(".mymoney span").append(balance);
    click();
});
function click()
{
    $(".butok").click(function(){
        window.location.href = '../view/payPassword.html';
    })
    $(".cancel").click(function() {
        $(".dialog").hide();
    });
}